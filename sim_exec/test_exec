    %vpi_call 2 18 "$display", "%0t\011%b\011%b\011%b\011%b\011%b\011%b", $time, v0x5a7ef70f0620_0, v0x5a7ef713ed80_0, v0x5a7ef713ecc0_0, v0x5a7ef713eee0_0, v0x5a7ef713ee20_0, v0x5a7ef713eff0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a7ef70f0620_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a7ef713ed80_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 21 "$display", "%0t\011%b\011%b\011%b\011%b\011%b\011%b", $time, v0x5a7ef70f0620_0, v0x5a7ef713ed80_0, v0x5a7ef713ecc0_0, v0x5a7ef713eee0_0, v0x5a7ef713ee20_0, v0x5a7ef713eff0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a7ef70f0620_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a7ef713ed80_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 24 "$display", "%0t\011%b\011%b\011%b\011%b\011%b\011%b", $time, v0x5a7ef70f0620_0, v0x5a7ef713ed80_0, v0x5a7ef713ecc0_0, v0x5a7ef713eee0_0, v0x5a7ef713ee20_0, v0x5a7ef713eff0_0 {0 0 0};
