:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5a7ef70f0490 .scope module, "basic_gates" "basic_gates" 2 1;
 .timescale 0 0;
L_0x5a7ef713f0b0 .functor AND 1, v0x5a7ef70f0620_0, v0x5a7ef713ed80_0, C4<1>, C4<1>;
L_0x5a7ef713f140 .functor OR 1, v0x5a7ef70f0620_0, v0x5a7ef713ed80_0, C4<0>, C4<0>;
L_0x5a7ef713f250 .functor NOT 1, v0x5a7ef70f0620_0, C4<0>, C4<0>, C4<0>;
L_0x5a7ef713f2c0 .functor XOR 1, v0x5a7ef70f0620_0, v0x5a7ef713ed80_0, C4<0>, C4<0>;
v0x5a7ef70f0620_0 .var "a", 0 0;
v0x5a7ef713ecc0_0 .net "and_out", 0 0, L_0x5a7ef713f0b0;  1 drivers
v0x5a7ef713ed80_0 .var "b", 0 0;
v0x5a7ef713ee20_0 .net "not_out", 0 0, L_0x5a7ef713f250;  1 drivers
v0x5a7ef713eee0_0 .net "or_out", 0 0, L_0x5a7ef713f140;  1 drivers
v0x5a7ef713eff0_0 .net "xor_out", 0 0, L_0x5a7ef713f2c0;  1 drivers
    .scope S_0x5a7ef70f0490;
T_0 ;
    %vpi_call 2 13 "$display", "Testing Basic Logic Gates" {0 0 0};
    %vpi_call 2 14 "$display", "Time\011a\011b\011AND\011OR\011NOT\011XOR" {0 0 0};
